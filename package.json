    "jest": "jest --coverage=false --changedSince=origin/master",
    "jest:update": "jest -u --coverage=false",
    "expo:login": "expo login --username $EXPO_USER --password $EXPO_PASSWORD",
    "build:publish": "./scripts/expo-publish.sh",
